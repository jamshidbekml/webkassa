generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Categories {
  id String @id @default(uuid()) @db.Uuid
  name String
  code Int @unique
}

model Contracts {
  id String @id @default(uuid()) @db.Uuid
  saleId Int
  contractId String
  phone String
  passportSeries String
  pinfl String
}

model Receipts {
  id String @id @default(uuid()) @db.Uuid

}
